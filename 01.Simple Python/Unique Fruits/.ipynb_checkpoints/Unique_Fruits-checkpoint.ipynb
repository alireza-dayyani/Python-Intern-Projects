   "source": [
    "import random\n",
    "fruits=[\"apple\",\"banana\",\"cherry\",\"kiwi\",\"orange\",\"melon\"]\n",
    "unique_pairs=[]\n",
    "while len(unique_pairs)<15:\n",
    "    pair=random.sample(fruits,2)\n",
    "    if pair not in unique_pairs and pair[::-1] not in unique_pairs:\n",
    "        unique_pairs.append(pair)\n",
    "print(unique_pairs)"
   ]
  }
